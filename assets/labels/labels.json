    "validationData": {
        "name": {
            "maxLength": {
                "rule": 50
            }
        },
        "mobile": {
            "maxLength": {
                "rule": 10,
                "msg": "Should be 10 Digits"
            },
            "type": "number",
            "patternCheck": {
                "rule": "^[6-9][0-9]*$",
                "msg": "Invalid Mobile Number"
            }
        },
        "designation": {
            "maxLength": {
                "rule": 25
            }
        },
        "employeeCode": {
            "maxLength": {
                "rule": 15
            }
        },
        "email": {
            "maxLength": {
                "rule": 50
            },
            "patternCheck": {
                "rule": "^\\w+([.-]?\\w+)@\\w+([.-]?\\w+)(\\.\\w{2,10})+$",
                "msg": "Invalid email"
            }
        }

    }
